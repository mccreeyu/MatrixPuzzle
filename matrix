#include <stdio.h>
int main()
{
	int n,m,k=1,i,j;
	int a[100][100]; 
	scanf("%d",&n);
	if(n%2==0)
		m=n/2;
	else
		m=n/2+1;
	for(i=0;i<m;i++)//måœˆ 
	{
		for(j=i;j<n-i;j++)
			a[i][j]=k;
		for(j=i+1;j<n-i;j++)
			a[j][n-1-i]=k;
		for(j=n-2-i;j>=i;j--)
			a[n-1-i][j]=k;
		for(j=n-2-i;j>=i+1;j--)
			a[j][i]=k;
			k++;
	}
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
			printf("%4d",a[i][j]);
		printf("\n");
	}
	return 0;
}
